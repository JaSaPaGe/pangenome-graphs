PREFIX : <https://raw.githubusercontent.com/JaSaPaGe/pangenome-graphs/main/shex.rdf#>
PREFIX MainSchema: <http://jasapage.org/pgd_schema#MainSchema/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX obo: <http://purl.obolibrary.org/obo/>
PREFIX sio: <http://semanticscience.org/resource/>
PREFIX efo: <http://www.ebi.ac.uk/efo/>
PREFIX evs: <http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#>
PREFIX edam: <http://edamontology.org/>
PREFIX sch: <https://schema.org/>
PREFIX wikidata: <http://www.wikidata.org/entity/>

:pangenomeShape {
  sch:title  xsd:string ?;
  sch:description  xsd:string ?;
  efo:EFO_0000532 [ obo:NCBITaxon_~ ] ;
  sch:targetPopulation xsd:string ?;
  sch:version xsd:string ?;
  MainSchema:reference @:assemblyShape ?;
  MainSchema:assemblies @:assemblyShape ?;
  MainSchema:authors @:authorsShape ;
  sch:WebSite IRI {0,3};
}

:authorsShape {
    obo:NCIT_C42781 xsd:string + ;
    obo:NCIT_C92453 xsd:string ?;
    obo:IAO_0000422 xsd:string ?;
    obo:NCIT_C19026 xsd:string ?;
    sio:SIO_001167 xsd:string ?;
}

:assemblyShape  {
    sio:SIO_000115 xsd:string ?;
    assemblySchema:sequences @:sequenceShape ?;
    sch:version xsd:string ?;
    efo:EFO_0002699 xsd:string ?;
    edam:data_2091 IRI {0,3};
    sio:SIO_001167 xsd:string ?;
}

:sequenceShape  {
    sio:SIO_000115 xsd:string;
    edam:data_2091 IRI {0,3};
    obo:NCIT_C172274 xsd:string;
    obo:NCIT_C155320 xsd:double OR xsd:integer {0,3};
}